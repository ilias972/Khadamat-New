{
  "name": "@khadamat/database",
  "version": "1.0.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "generate": "dotenv -e ../../.env.database -- prisma generate",
    "migrate": "dotenv -e ../../.env.database -- prisma migrate dev",
    "db:push": "dotenv -e ../../.env.database -- prisma db push",
    "studio": "dotenv -e ../../.env.database -- prisma studio",
    "validate": "prisma validate",
    "seed": "dotenv -e ../../.env.database -- tsx prisma/seed.ts"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "5.17.0",
    "bcryptjs": "^2.4.3",
    "pg": "^8.17.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^20.19.30",
    "@types/pg": "^8.16.0",
    "bcryptjs": "^3.0.3",
    "prisma": "5.17.0",
    "tsx": "^4.21.0"
  }
}
